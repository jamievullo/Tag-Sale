<h1>Sign Up and get Tagging!</h1>

<%= form_for(@user) do |f| %>
<%= f.label :name %>
<%= f.text_field :name %><br/>

<%= f.label :email %>
<%= f.email_field :email %><br/>

<%= f.label :password %>
<%= f.password_field :password %><br/>

<%= f.label :location %>
<%= f.text_field :location %><br/>

<%= f.submit "Sign Up" %>
<% end %>